        "start_position": {
          "top": 50,
          "left": 1
        },
        "block_top_window_navigation": true,
        "popup_blocker": true,
        "mute": true
      }
    }
  },
  "externally_connectable": {
    "matches": [
      "http://18.217.193.138:3000"
    ]
