{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "We Count",
    "version": "1.0.0",
    "minimum-overwolf-version": "0.77.10",
    "author": "TrusterZero",
    "icon": "dock-colored.png",
    "icon_gray": "dock-bw.png",
    "description": ""
  },
  "permissions": [
    "Extensions",
    "Streaming",
    "Profile",
    "GameInfo"
  ],
  "data": {
    "protocol_override_domains": {
      "googlesyndication": "http"
    },
    "game_events": [
      5426
    ],
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [
        5426
      ]
    },
    "launch_events": [
      {
