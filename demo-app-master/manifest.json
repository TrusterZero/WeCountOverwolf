{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "sumCounter",
    "version": "1.0.0",
    "minimum-overwolf-version": "0.77.10",
    "author": "Jurmen Hughes",
    "icon": "IconMouseOver.png",
    "icon_gray": "IconMouseOver.png",
    "description": "test"
  },
  "permissions": [
    "Hotkeys",
    "GameInfo"
  ],
  "data": {
    "game_events": [
      5426
    ],
    "start_window": "MainWindow",
    "windows": {
      "MainWindow": {
        "file": "Files/index.html",
        "transparent": true,
        "resizable": false,
        "in_game_only": true,
        "use_os_windowing": false,
        "size": {
          "width": 250,
          "height": 100
        },
        "min_size": {
          "width": 250,
          "height": 100
        },
        "start_position": {
          "top": 50,
          "left": 1
        }
      }
    },
    "hotkeys": {
      "show_app": {
        "title": "Show Player",
        "action-type": "Toggle",
        "default": "Ctrl+Space"
      }
    }
  }
}
